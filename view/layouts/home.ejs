<div class="row mb-5">
    <div class="col-lg-6 col-md-12 mb-3">
        <% if(Users){ %>
            <div class="card ">
                <div class="card-header">
                    <h4 class="card-title text-center"> Usuarios</h4>

                </div>
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table tablesorter " id="">
                            <thead class=" text-primary">
                                <tr>
                                    <th>ID</th>
                                    <th>USUARIO</th>
                                    <th>Nombre</th>
                                    <th>ROL</th>

                                </tr>
                            </thead>
                            <tbody>
                                <% Users.forEach(user => { %>
                                    <tr>
                                        <td>
                                            <%= user.id %>
                                        </td>
                                        <td>
                                            <%= user.email %>
                                        </td>
                                        <td>
                                            <%= user.name %>
                                        </td>
                                        <td>
                                            <%= user.rol %>
                                        </td>
                                    </tr>
                                    <% }); %>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
            <% } %>
    </div>
    <% if(Events){ %>
        <div class="col-lg-6 col-md-12">
            <div class="card ">
                <div class="card-header">
                    <h4 class="card-title text-center"> Eventos</h4>
                </div>
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table tablesorter " id="">
                            <thead class=" text-primary">
                                <tr>
                                    <th>titulo</th>
                                    <th>Descripcion</th>
                                    <th>Fecha</th>
                                    <th>Hora</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% Events.forEach(Event => { %>
                                    <tr>
                                        <td>
                                            <%= Event.title %>
                                        </td>
                                        <td>
                                            <%= Event.short_description %>
                                        </td>
                                        <td>
                                            <%= Event.fecha %>
                                        </td>
                                        <td>
                                            <%= Event.hora %>
                                        </td>
                                    </tr>
                                    <% }); %>
                            </tbody>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
        <% } %>
</div>
<div class="row">
    <% if(Blogs){ %>
        <div class="col-lg-6 col-md-12 mb-3">
            <div class="card ">
