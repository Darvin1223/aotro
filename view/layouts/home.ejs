                                    <th>titulo</th>
                                    <th>Descripcion</th>

                                </tr>
                            </thead>
                            <tbody>
                                <% Blogs.forEach(Blog => { %>
                                    <tr>
                                        <td>
                                            <%= Blog.title %>
                                        </td>
                                        <td>
                                            <%= Blog.short_description %>
                                        </td>

                                    </tr>
                                    <% }); %>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
        <% } %>
